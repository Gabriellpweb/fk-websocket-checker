{
  "endpoint": "ws://mywebsocket.endpoint",
  "send": "myrequestforwebsocket",
  "timeout": 5,
  "total_frames": 5,
  "assert_rate": 30,
  "validations": [
    {
      "type": "regex",
      "value": "^Connected"
    },
    {
      "type": "regex",
      "value": "^myexpression"
    }
  ],
  "on_success": [
    {
      "type": "mail"
    }
  ],
  "on_fail": [
    {
      "type": "mail"
    }
  ],
  "smtp": {
    "host": "smtp.mymail.com",
    "port": "587",
    "username": "myuser@mail.com",
    "password": "mailpassword",
    "from": "devops@mail.com",
    "to": [
      "receiver@mail.com"
    ]
  }
}